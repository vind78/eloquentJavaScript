function reversearray(array){
	arraylength = array.length;
	var arr = array; var j  = arraylength;
	
	for (i=0;i<arraylength;i++)
		{
		//a[i] = arr[j-1];
		
		console.log(arr[j-1]);
		console.log(a[i]);
		a[i]=arr[j-1];
		j-=1
		}
	
	return (a);
	
}
var a = [1 ,2 ,3, 4];
reversearray(a);
