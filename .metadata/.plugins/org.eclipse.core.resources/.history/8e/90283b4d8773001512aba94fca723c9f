/**
 * 
 */
var arr = [];
function range(start,end,step){
	
	for (i=start;i<=end;i+=2){
		arr[i-1] = i;		
	}
	return arr;
}
//sumarr(range(1,10));
console.log(sumarr(range(1,10)));

function sumarr(array){
	var c = 0;
	for(i=0;i<array.length;i++){
		
		c+=array[i];
	}
	return(c);

}
//console.log(sumarr(range(1,10)));